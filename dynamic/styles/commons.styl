strong
    font-weight: 500

.page-header
    margin-bottom: var(--two-units)

#page-heading
    margin-top: var(--three-units)

#page-title
    font-size: var(--xxxl)
    font-family: heading-font
    margin-bottom: var(--unit)
    line-height: 1.1    

#page-subtitle
    font-size: var(--xxl)
    margin-top: 0.1em
    display: block
    color: primary

#page-main
    margin-top: var(--two-units)
    position: relative

.page-footer
    background: white
    margin-top: var(--two-units)
    padding: var(--two-units) 0


.s1 + .s1
    margin-top: var(--unit)

.s1
    address + iframe
        margin-top: var(--unit)

.s1-header + .s1-main
    margin-top: var(--semi-unit)

.h2
    font-size: var(--l)
    font-family: heading-font
    font-weight: 500

.h3
    font-weight: 500
    font-family: heading-font